{
  "name": "dishjs",
  "version": "0.0.1",
  "main": "index.js",
  "unpkg": "dist/dish.js",
  "files": [
    "index.js",
    "libs",
    "dist"
  ],
  "scripts": {
    "lint": "eslint --ignore-pattern '/dist/' '**/*.js'",
    "test": "istanbul cover node_modules/jasmine/bin/jasmine.js -- JASMINE_CONFIG_PATH=jasmine.json",
    "upload-coverage": "cat coverage/lcov.info | node_modules/coveralls/bin/coveralls.js",
    "build": "npm run build:umd && npm run build:umd-min",
    "build:umd": "rollup -c",
    "build:umd-min": "rollup -c --environment MIN",
    "prepublish": "npm run build"
  },
  "devDependencies": {
    "coveralls": "^2.12.0",
    "eslint": "^3.17.0",
    "eslint-config-standard": "^7.0.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^2.1.1",
    "istanbul": "^0.4.5",
    "jasmine": "^2.5.3",
    "rollup": "^0.41.6",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-uglify": "^1.0.1"
  },
  "author": "Mingkai Li (https://github.com/lmk123)",
  "license": "MIT"
}
